  "description": "Simple function based component library for hyperx/bel/halfcab tagged template literals",
  "main": "index.mjs",
  "module": "index.mjs",
  "jsnext:main": "index.mjs",
  "@std/esm": true,
  "esm": true,
  "scripts": {
    "test": "npm run test:lint",
