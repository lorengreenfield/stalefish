  "bugs": {
    "url": "https://github.com/lorengreenfield/stalefish/issues"
  },
  "homepage": "https://github.com/lorengreenfield/stalefish#readme",
  "peerDependencies": {
    "halfcab": "^10.2.2"
  },
  "devDependencies": {
    "halfcab": "^10.2.2",
    "standard": "^11.0.1"
  },
  "dependencies": {
    "flatpickr": "^4.5.1",
    "hash-it": "^4.0.1"
